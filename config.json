{
  "app": {
    "name": "E-commerce System",
    "version": "1.0.0",
    "environment": "development"
  },
  "features": {
    "enableCache": true,
    "enableThrottling": true,
    "enableCompression": true,
    "maxPageSize": 50
  },
  "limits": {
    "maxOrderItems": 20,
    "maxSearchResults": 100,
    "cacheExpirationMinutes": 5
  }
}